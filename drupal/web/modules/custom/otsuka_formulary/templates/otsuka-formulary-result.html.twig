{#
/**
 * @file
 * Default theme implementation to display a formulary result.
 *
 * Variables:
 * - components
 * - form
 * - mailstring
 */
#}
<div class="formulary-results">
{{ components }}
</div>
<div class="formulary-widgets">
  <div class="row email-widget">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      {{ form }}
    </div>
  </div>
  <div class="formulary-results-email-data hidden">
    {{- mailstring -}}
  </div>
</div>
